#include<iostream>
using namespace std;

int bubble(int *arr,int n){
	if( n==0 || n== 1){
		return 1;                    //base case
	}
	
	for(int i=1;i<n;i++){
		if(arr[i-1] > arr[i])
			swap(arr[i-1],arr[i]);
	}
	bubble(arr, n-1);	
}
int main(){
	int arr[10] = {1,3,9,0,3,5,3,2,7,4};
	
	bubble(arr,10);
	
	for(int i=0;i<10;i++){
		cout<<arr[i]<<" ";
	}
}
